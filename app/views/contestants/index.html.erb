<h1>All Contestants</h1>

<% @contestants.each do |contestant| %>
  <section id="contestant-<%= contestant.id %>">
    <h2><%= contestant.name %></h2>
      <h4>Projects:</h4>
        <ul>
          <% contestant.projects.each do |project| %>
            <li><%= project.name %>
          <% end %>
        </ul>
  </section>
<% end %>
